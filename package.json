{
  "name": "yayoi-js",
  "version": "1.1.0",
  "description": "Image super-resolution using deep convolutional neural network (CNN)",
  "private": true,
  "author": "Panepo <panepo@github.io>",
  "homepage": "https://panepo.github.io/Yayoi-js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Panepo/Yayoi-js.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn prettier && yarn build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/",
    "prettier": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.{js,jsx,json,css}\"",
    "precommit": "lint-staged && yarn build-css",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "yarn build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --no-semi --trailing-comma none --write",
      "git add"
    ]
  },
  "dependencies": {
    "@tensorflow/tfjs": "^0.12.3",
    "@tensorflow/tfjs-converter": "^0.5.4",
    "@tensorflow/tfjs-core": "^0.12.7",
    "babel-polyfill": "^6.26.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2017": "^6.24.1",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "gh-pages": "^1.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "node-sass-chokidar": "^1.3.3",
    "prettier": "^1.13.7"
  }
}
