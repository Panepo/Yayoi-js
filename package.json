{
  "name": "yayoi-js",
  "version": "1.0.0",
  "description": "Image super-resolution using deep convolutional neural network (CNN)",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development lsc server.ls --nodejs --harmoy",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config webpack.config.prod.ls",
    "travis": "cross-env NODE_ENV=production webpack --config webpack.config.prod.ls",
    "dev": "npm run clean && cross-env NODE_ENV=development webpack --config webpack.config.dev.ls",
    "test": "mocha --compilers js:babel-core/register",
    "lint": "eslint ./src/",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npm run prettierJS && npm run prettierJSX",
    "prettierJS": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.js\"",
    "prettierJSX": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.jsx\""
  },
  "author": "Panepo <panepo@github.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Panepo/Yayoi-js.git"
  },
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-redux": "^4.4.9",
    "redux": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "8.2.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.9.1",
    "eslint-plugin-standard": "^3.1.0",
    "express": "^4.16.3",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.4",
    "livescript": "^1.4.0",
    "livescript-loader": "^0.1.5",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.0",
    "prettier": "1.11.1",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^3.2.0",
    "should": "^13.2.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.22.2"
  }
}
